#------------------------------------------------------------------
# Sina's Test Production Rules (this time in SPARQL form!)
# A really simple set of production rules 
#------------------------------------------------------------------
REGISTER QUERY Test AS
PREFIX  dc:  <http://purl.org/dc/elements/1.1/>
PREFIX  ns:  <http://example.org/ns#>

SELECT  ?p
FROM STREAM <file:///Users/ss/Development/java/openimaj/trunk/storm/ReteStorm/src/test/resources/test.groupby.rdfs> [RANGE 30m STEP 5m]
{  ?x ns:price ?p .
   ?x ns:discount ?discount .
   FILTER(?p * ?discount < 20) .
   ?x dc:title ?title . 
} GROUP BY ?p